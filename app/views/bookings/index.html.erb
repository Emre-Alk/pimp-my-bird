<div class="container">
  <h1>Bookings</h1>
  <%@bookings.each do |booking| %>
    <div class="card-product">
    <%= cl_image_tag booking.bird.picture.key, crop: :fit, gravity: :center %>
    <div class="card-product-infos">
      <h2><%= link_to booking.bird.name, bird_path(booking.bird) %></h2>
      From <span> <%= booking.start_date %> </span> to <span> <%= booking.end_date %> </span>
    </div>
  </div>
  <% end %>
</div>
